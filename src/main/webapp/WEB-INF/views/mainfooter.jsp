    });
    $('#main-projects-keyvisual').bannerSlide({
      fade: !('ontouchstart' in window),
      autoplay: true,
      autoplaySpeed: 5000
    });



    /*wadizPicksInvest*/
    $('.wadizpicks-list > ul, #programEventPicks > ul, .exhibition-list > ul, #wadizCastIntro .wadizcast-list > ul').slick({
        infinite: true,
        dots:true,
