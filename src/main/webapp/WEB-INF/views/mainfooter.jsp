        autoplay: false,
        speed: 100
    });

    $.each($('.slick-slider'), function(idx){
      var slickOpts = {
          infinite: true,
              dots:true
      }

      if($(this).find('.slick-slide').length == 1){
        $(this).slick('slickSetOption', 'slidesToShow', 0 , true).addClass('none-arr');
